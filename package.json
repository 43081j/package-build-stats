{
  "name": "package-build-stats",
  "version": "1.1.1",
  "author": "Shubham Kanodia <shubham.kanodia10@gmail.com>",
  "repository": "https://github.com/pastelsky/package-build-stats",
  "license": "MIT",
  "engines": {
    "node": ">=6.x.x",
    "npm": ">=3.x.x"
  },
  "files": [
    "getPackageStats.js",
    "CustomError.js",
    "utils",
    "config.js"
  ],
  "main": "./getPackageStats.js",
  "scripts": {
    "start": "DEBUG=bp:* sls offline start",
    "test": "ava",
    "show-logs": "sls logs --tail -f getPackageStats",
    "replace-sass-bindings": "rm -rf ./node_modules/node-sass/vendor/* && cp -R ./utils/linux-x64-48 ./node_modules/node-sass/vendor/",
    "restore-sass-bindings": "rm -rf ./node_modules/node-sass/vendor/* && npm rebuild node-sass",
    "deploy": "yarn run replace-sass-bindings && sls deploy && yarn run restore-sass-bindings"
  },
  "ava": {
    "files": [
      "tests/*.js"
    ]
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "dotenv": "^4.0.0",
    "p-series": "^1.0.0",
    "serverless-offline": "^3.15.3",
    "write-file-webpack-plugin": "^4.1.0"
  },
  "dependencies": {
    "autoprefixer": "^7.1.3",
    "css-loader": "^0.28.7",
    "debug": "^3.0.1",
    "escape-string-regexp": "^1.0.5",
    "extract-text-webpack-plugin": "^3.0.0",
    "memory-fs": "^0.4.1",
    "mkdir-promise": "^1.0.0",
    "node-fetch": "^1.7.2",
    "node-sass": "^4.5.3",
    "pify": "^3.0.0",
    "postcss-loader": "^2.0.6",
    "sanitize-filename": "^1.6.1",
    "sass-loader": "^6.0.6",
    "webpack": "^3.5.5",
    "webpack-parallel-uglify-plugin": "^1.0.0"
  }
}
